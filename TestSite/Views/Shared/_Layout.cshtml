<!DOCTYPE HTML>
<html>
<head>
    @await Html.PartialAsync("MetaTagsPartial")
    @await Html.PartialAsync("CssPartial")
</head>
<body>
    <div id="page-wrapper">
        @await Html.PartialAsync("HeaderPartial")
        <div id="main">
            <div class="container">
                <a asp-controller="Account" asp-action="Register" class="buttonReg">Зарегистрировать компанию</a>
                <a asp-controller="Account" asp-action="Login" class="buttonReg">Войти</a>
                @RenderBody()
            </div>
            @await Html.PartialAsync("SidebarPartial")
        </div>
    </div>
    @await Html.PartialAsync("FooterPartial")
    @await Html.PartialAsync("ScriptsPartial")
</body>
</html>